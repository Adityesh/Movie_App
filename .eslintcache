       setShows(false);\n        }\n\n        setMovie(true);\n    }\n\n    const openShowModal = () => {\n        if(movie) {\n            setMovie(false);\n        }\n\n        setShows(true);\n    }\n    \n\n\n    return (\n        <div style={{ height: '100vh', width: '100%'}}>\n            <Draggable bounds=\"parent\">\n                <div style={{ display: 'inline-block', margin : '20px'}} id=\"movie\" onClick={openMovieModal}>\n                    <img src={movieIcon} height={48} width={48} alt=\"Movie Icon\" draggable=\"false\" className=\"movie\"/>\n                    <p style={{color : \"white\"}} className=\"movie\">Movies</p>\n                </div>\n            </Draggable>\n            <Draggable bounds=\"parent\">\n                <div style={{ display: 'inline-block', margin : '20px'}} id=\"show\" onClick={openShowModal}>\n                    <img src={showIcon} height={48} width={48} alt=\"Movie Icon\" draggable=\"false\" className=\"show\"/>\n                    <p style={{color : \"white\"}} className=\"show\">TV Shows</p>\n                </div>\n            </Draggable>\n            <Movies isVisible={movie} closeModal={setMovie}/>\n            <Shows isVisible={shows} closeModal={setShows}/>\n            \n        </div>\n    )\n}\n\nexport default Home;","/home/adityesh/movieapp/src/components/Content/Movies/Movies.js",["58"],"import React, { useState } from 'react';\nimport './Movies.css';\nimport Draggable from 'react-draggable';\nimport { Tab, Tabs, TabList, TabPanel } from 'react-tabs';\nimport 'react-tabs/style/react-tabs.css';\n\nconst NowPlaying = React.lazy(() => import('./MovieComponents/NowPlaying'));\nconst Popular = React.lazy(() => import('./MovieComponents/Popular'));\nconst Search = React.lazy(() => import('./MovieComponents/SearchMovie'));\n\n\nconst Movies = ({ isVisible, closeModal }) => {\n\n\n\n    return (\n        <Draggable bounds=\"parent\" handle=\"strong\">\n\n\n            <div className=\"movies-modal window\" style={{ display: isVisible ? 'block' : 'none' }}>\n                <strong>\n                <div class=\"title-bar\">\n                    <div class=\"title-bar-text\">Movies</div>\n                    <div class=\"title-bar-controls\">\n                        \n                        <button aria-label=\"Close\" onClick={() => closeModal(false)}></button>\n                    </div>\n                </div>\n                </strong>\n\n                <div className=\"window-body\">\n                    <Tabs>\n                        <TabList>\n                            <Tab>Now Playing</Tab>\n                            <Tab>Popular Movies</Tab>\n                            <Tab>Search</Tab>\n                        </TabList>\n\n                        <TabPanel>\n                            <NowPlaying/>\n                        </TabPanel>\n                        <TabPanel>\n                            <Popular />\n                        </TabPanel>\n                        <TabPanel>\n                            <Search />\n                        </TabPanel>\n                    </Tabs>\n                </div>\n\n\n            </div>\n\n        </Draggable>\n    )\n}\n\n\nexport default Movies;","/home/adityesh/movieapp/src/components/Content/Shows/Show.js",[],"/home/adityesh/movieapp/src/components/Loader/Loader.js",[],"/home/adityesh/movieapp/src/components/Content/Movies/MovieComponents/NowPlaying.js",["59"],"/home/adityesh/movieapp/src/components/Content/Movies/MovieComponents/Popular.js",["60"],"import React, {useState, useEffect} from 'react';\nimport Loader from '../../../Loader/Loader';\nimport './Movie.css';\nimport { fetchMovies } from '../../../../utils/fetchMovies';\nimport Pagination from '../../../Pagination';\n\nconst Popular = () => {\n    const [isLoading, setLoading] = useState(false);\n    const [results, setResults] = useState([]);\n    const [page, setPage] = useState(1);\n    const [totalPage, setTotalPage] = useState(1);\n\n    const imageUrl = (path) => {\n        const fullPath = `https://image.tmdb.org/t/p/w185${path}`;\n        return fullPath;\n    }\n\n    useEffect(() => {\n        setLoading(true)\n        fetchMovies('/movie/popular', page).then(({error, message, result, totalPages}) => {\n            setResults(result);\n            setTotalPage(totalPages);\n            setLoading(false);\n        }).catch((err) => {\n            console.log(err);\n        })\n\n    }, [page])\n\n    const fetchNewPage = () => {\n        setLoading(true)\n        fetchMovies('/movie/now_playing', page).then(({error, message, result}) => {\n            setResults(result);\n            setLoading(false);\n        }).catch((err) => {\n            console.log(err);\n        })\n    }\n\n    \n\n    return(\n        <div>\n            <p>Page {page} of {totalPage}</p>\n            {isLoading ? <Loader /> : null}\n            <div className=\"movie-container\">\n            \n            {results.length !== 0 ? results.map((movie, index) => {\n                return (\n                    <div key={index} className=\"movie-element\">\n                        <img src={imageUrl(movie.poster_path)} alt=\"image for Movie\" />\n                        <p>{movie.title} - {movie.release_date.substring(0,4)}</p>\n                        \n                        </div>\n                )\n            }) : null}\n            </div>\n            \n\n            <Pagination \n            totalPages={totalPage} \n            currentPage={page}\n            setPage={setPage}\n            fetchMovies={fetchNewPage}\n            />\n        </div>\n    )\n}\n\nexport default Popular;","/home/adityesh/movieapp/src/components/Content/Movies/MovieComponents/SearchMovie.js",["61","62"],"import React, {useState, useEffect} from 'react';\n\nconst SearchMovie = () => {\n    return(\n        <div>\n            SearchMovie Component\n        </div>\n    )\n}\n\nexport default SearchMovie;","/home/adityesh/movieapp/src/utils/